// trivial solution 

function vowelsAndConsonants(s) {
    let vowels = ('aiou').split('');

    for (let v of s) {
        if (vowels.includes(v))
            console.log(v)
    }
    for (let v of s) {
        if (!vowels.includes(v))
            console.log(v)
    }
}

console.log(vowelsAndConsonants('javascriptloops'))
