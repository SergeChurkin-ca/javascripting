// this is the pattern
//  # # # # #
// # # # # # 
//  # # # # #

let length = 10;
let board = "";

for (let i = 0; i < length; i++) {
    for (let x = 0; x < length; x++) {

        if ((i + x) % 2 == 0) {
            board += " ";
        } else {
            board += '#'
        }
    }
    board += "\n"
}

console.log(board)
