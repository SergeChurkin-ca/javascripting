// sortDrinkByPrice(drinks) âžž [{name: "lime", price: 10}, {name: "lemonade", price: 50}]

drinks = [
    { name: "lemonade", price: 50 },
    { name: "lime", price: 10 }
]

function sortDrinkByPrice(drinks) {
    return drinks.sort((a, b) => a.price - b.price)
}

console.log(sortDrinkByPrice(drinks))
